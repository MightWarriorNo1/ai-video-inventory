# PostgreSQL connection string (required)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/trailer_vision

# Port for Flask (default 8080)
PORT=8080

# Optional: require X-API-Key header for ingest (set same value on device as DASHBOARD_API_KEY)
# DASHBOARD_API_KEY=your-secret-key

# Optional: serve React dashboard static files (path to yardvision-dashboard/dist)
# DASHBOARD_STATIC_DIR=C:\path\to\yardvision-dashboard\dist

# Optional: directory for uploaded cropped images (default: ./uploads)
# UPLOAD_DIR=./uploads
