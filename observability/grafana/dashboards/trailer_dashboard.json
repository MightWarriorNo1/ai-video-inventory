{
  "dashboard": {
    "title": "Trailer Vision Edge - Dashboard",
    "tags": ["trailer", "edge", "vision"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "FPS per Camera",
        "type": "graph",
        "targets": [
          {
            "expr": "trailer_vision_fps",
            "legendFormat": "{{camera_id}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Frames Processed",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(trailer_vision_frames_processed_total[5m])",
            "legendFormat": "{{camera_id}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Queue Depth",
        "type": "graph",
        "targets": [
          {
            "expr": "trailer_vision_queue_depth"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Events Logged",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(trailer_vision_events_logged_total[5m])"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      }
    ],
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1
  }
}


